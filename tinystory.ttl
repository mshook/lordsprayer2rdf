@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix research: <http://example.org/research/schema#> .
@prefix inst: <http://example.org/research/instance#> .

# ============================================
# RDFS Schema Definitions
# ============================================

# Classes
research:Paper a rdfs:Class ;
    rdfs:label "Research Paper" ;
    rdfs:comment "An academic research paper or work" .

research:Dataset a rdfs:Class ;
    rdfs:label "Dataset" ;
    rdfs:comment "A collection of data used for research or training" .

research:Method a rdfs:Class ;
    rdfs:label "Research Method" ;
    rdfs:comment "A methodology or approach used in research" .

research:Finding a rdfs:Class ;
    rdfs:label "Research Finding" ;
    rdfs:comment "A result or discovery from the research" .

research:Contribution a rdfs:Class ;
    rdfs:label "Research Contribution" ;
    rdfs:comment "A novel contribution or innovation introduced by the research" .

research:Model a rdfs:Class ;
    rdfs:label "Model" ;
    rdfs:comment "A computational model or language model" .

research:Capability a rdfs:Class ;
    rdfs:label "Capability" ;
    rdfs:comment "An ability or capability demonstrated by a model or system" .

# Properties for Paper
research:hasTitle a rdf:Property ;
    rdfs:label "has title" ;
    rdfs:comment "The title of the paper" ;
    rdfs:domain research:Paper ;
    rdfs:range rdfs:Literal .

research:introducesDataset a rdf:Property ;
    rdfs:label "introduces dataset" ;
    rdfs:comment "A dataset introduced by the paper" ;
    rdfs:domain research:Paper ;
    rdfs:range research:Dataset .

research:usesMethod a rdf:Property ;
    rdfs:label "uses method" ;
    rdfs:comment "A method or approach used in the research" ;
    rdfs:domain research:Paper ;
    rdfs:range research:Method .

research:hasFinding a rdf:Property ;
    rdfs:label "has finding" ;
    rdfs:comment "A finding or result from the research" ;
    rdfs:domain research:Paper ;
    rdfs:range research:Finding .

research:hasContribution a rdf:Property ;
    rdfs:label "has contribution" ;
    rdfs:comment "A contribution made by the research" ;
    rdfs:domain research:Paper ;
    rdfs:range research:Contribution .

research:hasAbstract a rdf:Property ;
    rdfs:label "has abstract" ;
    rdfs:comment "The abstract text of the paper" ;
    rdfs:domain research:Paper ;
    rdfs:range rdfs:Literal .

# Properties for Dataset
research:hasName a rdf:Property ;
    rdfs:label "has name" ;
    rdfs:comment "The name of the dataset or resource" ;
    rdfs:domain rdfs:Resource ;
    rdfs:range rdfs:Literal .

research:hasDescription a rdf:Property ;
    rdfs:label "has description" ;
    rdfs:comment "A description of the resource" ;
    rdfs:domain rdfs:Resource ;
    rdfs:range rdfs:Literal .

research:contains a rdf:Property ;
    rdfs:label "contains" ;
    rdfs:comment "What the dataset contains" ;
    rdfs:domain research:Dataset ;
    rdfs:range rdfs:Literal .

research:generatedBy a rdf:Property ;
    rdfs:label "generated by" ;
    rdfs:comment "The tool or model that generated the dataset" ;
    rdfs:domain research:Dataset ;
    rdfs:range rdfs:Literal .

research:targetedAt a rdf:Property ;
    rdfs:label "targeted at" ;
    rdfs:comment "The intended audience or use case" ;
    rdfs:domain research:Dataset ;
    rdfs:range rdfs:Literal .

# Properties for Method
research:involves a rdf:Property ;
    rdfs:label "involves" ;
    rdfs:comment "What the method involves" ;
    rdfs:domain research:Method ;
    rdfs:range rdfs:Literal .

# Properties for Finding
research:demonstrates a rdf:Property ;
    rdfs:label "demonstrates" ;
    rdfs:comment "What the finding demonstrates" ;
    rdfs:domain research:Finding ;
    rdfs:range rdfs:Literal .

research:concernsModel a rdf:Property ;
    rdfs:label "concerns model" ;
    rdfs:comment "A model that the finding is about" ;
    rdfs:domain research:Finding ;
    rdfs:range research:Model .

# Properties for Model
research:hasParameter a rdf:Property ;
    rdfs:label "has parameter" ;
    rdfs:comment "A parameter specification of the model" ;
    rdfs:domain research:Model ;
    rdfs:range rdfs:Literal .

research:hasArchitecture a rdf:Property ;
    rdfs:label "has architecture" ;
    rdfs:comment "The architecture of the model" ;
    rdfs:domain research:Model ;
    rdfs:range rdfs:Literal .

research:produces a rdf:Property ;
    rdfs:label "produces" ;
    rdfs:comment "What the model produces or outputs" ;
    rdfs:domain research:Model ;
    rdfs:range rdfs:Literal .

research:hasCapability a rdf:Property ;
    rdfs:label "has capability" ;
    rdfs:comment "A capability the model possesses" ;
    rdfs:domain research:Model ;
    rdfs:range research:Capability .

# Properties for Capability
research:hasType a rdf:Property ;
    rdfs:label "has type" ;
    rdfs:comment "The type or category" ;
    rdfs:domain rdfs:Resource ;
    rdfs:range rdfs:Literal .

# ============================================
# Paper Instance: TinyStories Paper
# ============================================

inst:TinyStoriesPaper a research:Paper ;
    rdfs:label "TinyStories Paper" ;
    research:hasTitle "TinyStories: How Small Can Language Models Be and Still Speak Coherent English?" ;
    research:hasAbstract "In this work, we introduce TinyStories, a synthetic dataset of short stories that only contain words that a typical 3 to 4-year-olds usually understand, generated by GPT-3.5 and GPT-4. We show that TinyStories can be used to train and evaluate LMs that are much smaller than the state-of-the-art models (below 10 million total parameters), or have much simpler architectures (with only one transformer block), yet still produce fluent and consistent stories with several paragraphs that are diverse and have almost perfect grammar, and demonstrate reasoning capabilities." ;
    research:introducesDataset inst:TinyStoriesDataset ;
    research:usesMethod inst:SyntheticGeneration ;
    research:hasFinding inst:Finding1, inst:Finding2, inst:Finding3 ;
    research:hasContribution inst:MainContribution .

# Dataset
inst:TinyStoriesDataset a research:Dataset ;
    rdfs:label "TinyStories Dataset" ;
    research:hasName "TinyStories" ;
    research:hasDescription "A synthetic dataset of short stories" ;
    research:contains "Short stories that only contain words that a typical 3 to 4-year-olds usually understand" ;
    research:generatedBy "GPT-3.5 and GPT-4" ;
    research:targetedAt "3 to 4-year-old vocabulary level" .

# Method
inst:SyntheticGeneration a research:Method ;
    rdfs:label "Synthetic Data Generation" ;
    research:hasDescription "Generating synthetic short stories using large language models" ;
    research:involves "Using GPT-3.5 and GPT-4 to create stories with controlled vocabulary" .

# Contribution
inst:MainContribution a research:Contribution ;
    rdfs:label "TinyStories Dataset Contribution" ;
    research:hasDescription "Introduction of TinyStories dataset for training and evaluating small language models" .

# Findings
inst:Finding1 a research:Finding ;
    rdfs:label "Small Model Capability" ;
    research:demonstrates "TinyStories can train LMs much smaller than state-of-the-art" ;
    research:concernsModel inst:SmallLM ;
    research:hasDescription "Models below 10 million parameters can be trained effectively" .

inst:Finding2 a research:Finding ;
    rdfs:label "Simple Architecture Sufficiency" ;
    research:demonstrates "Models with simpler architectures can produce quality output" ;
    research:concernsModel inst:SimpleArchLM ;
    research:hasDescription "Models with only one transformer block can be effective" .

inst:Finding3 a research:Finding ;
    rdfs:label "Output Quality" ;
    research:demonstrates "Small models can produce high-quality, coherent stories" ;
    research:hasDescription "Models produce fluent, consistent, diverse stories with near-perfect grammar" .

# Models
inst:SmallLM a research:Model ;
    rdfs:label "Small Language Model" ;
    research:hasDescription "Language models with fewer than 10 million total parameters" ;
    research:hasParameter "Below 10 million total parameters" ;
    research:produces "Fluent and consistent stories with several paragraphs" ;
    research:hasCapability inst:Fluency, inst:Consistency, inst:Grammar, inst:Reasoning .

inst:SimpleArchLM a research:Model ;
    rdfs:label "Simple Architecture Language Model" ;
    research:hasDescription "Language models with simplified architectures" ;
    research:hasArchitecture "Only one transformer block" ;
    research:produces "Fluent and consistent stories" ;
    research:hasCapability inst:Fluency, inst:Consistency .

# Capabilities
inst:Fluency a research:Capability ;
    rdfs:label "Fluency" ;
    research:hasType "Language production quality" ;
    research:hasDescription "Ability to produce fluent text" .

inst:Consistency a research:Capability ;
    rdfs:label "Consistency" ;
    research:hasType "Coherence" ;
    research:hasDescription "Ability to maintain consistency across paragraphs" .

inst:Grammar a research:Capability ;
    rdfs:label "Grammar" ;
    research:hasType "Linguistic correctness" ;
    research:hasDescription "Almost perfect grammatical correctness" .

inst:Reasoning a research:Capability ;
    rdfs:label "Reasoning" ;
    research:hasType "Cognitive capability" ;
    research:hasDescription "Demonstrated reasoning capabilities" .

inst:Diversity a research:Capability ;
    rdfs:label "Diversity" ;
    research:hasType "Output variety" ;
    research:hasDescription "Ability to produce diverse stories" .
